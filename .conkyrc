own_window yes
own_window_transparent yes
own_window_type desktop
own_window_hints undecorated,below,sticky,skip_taskbar,skip_pager
own_window_argb_visual true
own_window_argb_value 0
out_to_console no
use_xft yes
xftfont cure:size=10
update_interval 2
cpu_avg_samples 2
net_avg_samples 2
double_buffer yes
maximum_width 300
draw_shades no
draw_outline no
draw_borders no
stippled_borders 1
border_width 10
default_color white
default_shade_color white
default_outline_color white
alignment top_right
gap_x 5
gap_y 480
use_spacer left
no_buffers yes
uppercase no

