bar-list = ["bar1"];

bar1:{
  font: "terminus12, FontAwesome 9";
  block-list: ["ya_workspace", "ya_cpu", "ya_thermal", "ya_mem", "pacmandisplay", "ya_bat", "ya_date"];
  position: "top";
  gap-horizontal: 25;     #width in pixels
  gap-vertical: 22;     #width in pixels
  height: 25;       #height
  background-color-argb: 0x0;
  underline-size: 2;
  overline-size: 1;
  slack-size: 4;
  ya_date:{
                exec: "YABAR_DATE";
                align: "center";
                fixed-size: 140;
                interval: 2;
                background-color-rgb:0xBDD8DA;
                foreground-color-rgb:0x666666;
                underline-color-rgb:0xAD235E;
                internal-prefix: "  ";
                internal-option1: "%a %d %b, %I:%M";
  }
  ya_mem:{
                exec: "YABAR_MEMORY";
                background-color-rgb:0xBDD8DA;
                foreground-color-rgb:0x666666;
                underline-color-rgb:0xAD235E;
                align: "center";
                fixed-size: 70;
                interval: 1;
                internal-prefix: " ";
  }
  ya_thermal: {
                exec: "YABAR_THERMAL";
                background-color-rgb:0xBDD8DA;
                foreground-color-rgb:0x666666;
                underline-color-rgb:0xAD235E;
                align: "center";
                fixed-size: 50;
                interval: 1;
                internal-option1: "thermal_zone0"; #Get NAME from /sys/class/NAME/temp
                internal-prefix: " ";
   }
   pacmandisplay:{
                background-color-rgb:0xBDD8DA;
                foreground-color-rgb:0x666666;
                underline-color-rgb:0xAD235E;
               exec: "upd='  ' && upd+=$(pacman -Qu | wc -l) && echo $upd";
               type: "periodic";
               align: "center";
               fixed-size: 50;
               interval: 120;
          }
          ya_cpu: {
                exec: "YABAR_CPU";
                background-color-rgb:0xBDD8DA;
                foreground-color-rgb:0x666666;
                underline-color-rgb:0xAD235E;
                align: "center";
                fixed-size: 70;
                interval: 1;
                internal-prefix: "  ";
                internal-suffix: "%";
         }
         ya_workspace: {
            exec: "YABAR_WORKSPACE";
            background-color-rgb:0xBDD8DA;
            foreground-color-rgb:0x666666;
            underline-color-rgb:0xAD235E;
            align: "center";
            fixed-size: 40;
      }
  ya_bat: {
    exec: "YABAR_BATTERY";
            background-color-rgb:0xBDD8DA;
            foreground-color-rgb:0x666666;
            underline-color-rgb:0xAD235E;
    align: "right";
    fixed-size: 70;
    interval: 1;
    internal-suffix: "%";
    internal-option1: "BAT1";
    internal-option2: "        ";
    #internal-spacing: true;
  }

}
